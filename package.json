{
  "name": "@forlagshuset/nestjs-message-dispatcher",
  "version": "1.0.8",
  "description": "Creates and send messages to nats",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fagbokforlaget/nestjs-message-dispatcher.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/issues"
  },
  "homepage": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/#readme",
  "author": "",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "resetMocks": true
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/testing": "^10.0.3",
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "10.x",
    "@nestjs/core": "10.x",
    "rxjs": "7.x"
  }
}
