{
  "name": "message_dispatcher",
  "version": "1.0.0",
  "description": "Creates and send messages to nats",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fagbokforlaget/nestjs-message-dispatcher.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/issues"
  },
  "homepage": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/#readme",
  "author": "",
  "dependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "rxjs": "^7.8.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "@nestjs/testing": "^9.4.2",
    "jest": "29.4.2",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "@types/jest": "^29.5.1"
  }
}
