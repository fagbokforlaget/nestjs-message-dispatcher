{
  "name": "@forlagshuset/nestjs-message-dispatcher",
  "version": "1.0.8",
  "description": "Creates and send messages to nats",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fagbokforlaget/nestjs-message-dispatcher.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/issues"
  },
  "homepage": "https://github.com/fagbokforlaget/nestjs-message-dispatcher/#readme",
  "author": "",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "resetMocks": true
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/testing": "^10.2.6",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.6",
    "@nestjs/core": "^10.2.6",
    "rxjs": "7.x"
  }
}
